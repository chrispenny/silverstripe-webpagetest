{
  "name": "chrispenny/silverstripe-webpagetest",
  "description": "Submit performance test requests to WebPageTest, and consume the results",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "webpagetest", "performance", "testing", "silverstripe"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Christopher David Penny",
      "email": "chris.penny@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/chrispenny/silverstripe-webpagetest/issues"
  },
  "require": {
    "php": "^7.1",
    "silverstripe/vendor-plugin": "^1.0",
    "silverstripe/framework": "^4@dev",
    "silverstripe/cms": "^4@dev",
    "guzzlehttp/guzzle": "^6.0",
    "guzzlehttp/psr7": "^1.0",
    "opis/json-schema": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "ChrisPenny\\WebPageTest\\": "src/",
      "ChrisPenny\\WebPageTest\\Tests\\": "tests/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
